<div class= 'container'>
  <div class="row">
    <table id="example" class="table table-striped table-bordered mt-3">
      <thead>
        <tr>
          <th>User Name</th>
          <th>User Role</th>
          <th>User Email</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |u| %>
          <tr>
            <td> <%= u.name %> </td>
            <td> <%= u.role %> </td>
            <td> <%= u.email %> </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
