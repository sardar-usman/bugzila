<div class= 'container'>
  <div class="row">
    <h2 class="mt-3 text-muted"> Assign Sqa </h2>
    <table id="example" class="table table-striped table-bordered mt-2">
      <thead>
        <tr>
          <th>Developer Name</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
         <% @sqa.each do |s| %>
          <tr>
            <td> <%= s.name %> </td>
             <td>
              <% if @project_user.exists?(s.id) %>
                <%= link_to "Remove Sqa", remove_sqa_project_path(user_id: s.id), method: :delete, data: {confirm: "Are you sure?"}, class: 'btn btn-danger btn-sm' %>
              <% else %>
                <%= link_to "Add Sqa", add_sqa_project_path(user_id: s.id), method: 'post', class: 'btn btn-success btn-sm' %>
              <% end %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <%= link_to "Back", :back %>
  </div>
</div>
